{
  "name": "JournalEntry",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "mood": {
      "type": "string",
      "enum": [
        "very_happy",
        "happy",
        "neutral",
        "sad",
        "very_sad"
      ]
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "title",
    "content",
    "date"
  ]
}
